{
  "name": "animetrenz-api",
  "version": "1.1.2",
  "description": "Unofficial - This API will get current top animes in AnimeTrendingz",
  "scripts": {
    "preinstall": "tsup"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "types": "./src/types.d.ts",
  "keywords": [
    "anime-trendingz-api",
    "anime-top-api",
    "anime-api",
    "api",
    "anime-season-api",
    "all-api-for-anime-trendingz"
  ],
  "author": "Toni",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.2",
    "firebase": "^9.15.0"
  },
  "url": "https://github.com/isekaimaou1109/anitrendz-api/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:isekaimaou1109/anitrendz-api.git"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "sourcemap": false,
    "format": [
      "esm",
      "cjs"
    ],
    "shims": true
  }
}

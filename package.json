{
  "name": "animetrenz-api",
  "version": "1.0.4",
  "description": "Unofficial - This API will get current top animes in AnimeTrendingz",
  "type": "module",
  "scripts": {
    "build-node-esm": "tsc --project tsconfig.esm.json",
    "build-node-cjs": "tsc --project tsconfig.cjs.json",
    "preinstall": "npm run build-node-esm && npm run build-node-cjs"
  },
  "exports": {
    "import": "./build/node/esm/index.js",
    "default": "./build/node/esm/index.js",
    "require": "./build/node/cjs/index.js",
    "node": "./build/node/cjs/index.js",
    "types": "./src/types.d.ts"
  },
  "keywords": [
    "anime-trendingz-api",
    "anime-top-api",
    "anime-api",
    "api",
    "anime-season-api",
    "all-api-for-anime-trendingz"
  ],
  "author": "Toni",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.2",
    "firebase": "^9.15.0"
  },
  "url": "https://github.com/isekaimaou1109/anitrendz-api/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:isekaimaou1109/anitrendz-api.git"
  },
  "devDependencies": {}
}
